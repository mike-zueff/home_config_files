set nocompatible

filetype plugin indent on
map <F4> :tab split<CR>
map <F5> :tabnew<CR>
map <F11> :cp<CR>
map <F12> :cn<CR>
map Q <Nop>
map q <Nop>
match ErrorMsg /\s\+$/
set autoindent
set backspace=indent,eol,start
set confirm
set encoding=utf-8
set expandtab
set fileencoding=utf-8
set fileencodings=utf-8
set fileformat=unix
set history=4096
set hlsearch
set ic
set incsearch
set mouse=a
set nobackup
set noswapfile
set ruler
set shiftwidth=2
set showcmd
set smartindent
set smarttab
set softtabstop=2
set t_Co=256
set tabstop=2
set wrap
syntax on

colorscheme slate
